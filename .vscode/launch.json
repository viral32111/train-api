{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch",
			"preLaunchTask": "Build",
			"program": "${workspaceFolder}/dist/index.js",
			"skipFiles": [
				"<node_internals>/**"
			],
			"outFiles": [
				"${workspaceFolder}/dist/**/*.js"
			],
			"internalConsoleOptions": "openOnSessionStart"
		}
	]
}
